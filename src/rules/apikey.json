{
  "apikey": {
    "description": "Variable names refering to API keys",
    "message": "API key",
    "severity": "MAJOR",
    "key": {
      "regex": ".*[A-Za-z0-9\\-\\_]+(key|token)$",
      "ignorecase": true
    },
    "value": {
      "regex": "^(?!.*[ ])",
      "ignorecase": true,
      "minlen": 16,
      "isUri": false
    },
    "similar": 0.35
  }
}
